{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "algolia/algoliasearch-client-php": "^1.25",
        "aliyuncs/oss-sdk-php": "^2.2",
        "apollopy/flysystem-aliyun-oss": "^1.2",
        "barryvdh/laravel-debugbar": "^2.3",
        "doctrine/cache": "^1.6",
        "doctrine/dbal": "^2.5",
        "encore/laravel-admin": "^1.5",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "^5.4",
        "intervention/image": "^2.3",
        "jeroennoten/laravel-adminlte": "^1.19",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "~2.1.0",
        "monolog/monolog": "^1.22",
        "overtrue/laravel-filesystem-qiniu": "^1.0",
        "overtrue/laravel-wechat": "~4.0",
        "overtrue/wechat": "~4.0",
        "pimple/pimple": "^3.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^1.5",
        "stevenyangecho/laravel-u-editor": "^1.4",
        "symfony/http-foundation": "^3.2",
        "toplan/laravel-sms": "~2.6",
        "zgldh/qiniu-laravel-storage": "^0.9.0",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files":[
            "app/Extends/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
